{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/server.js"
    }
  ],
  "functions": {
    "backend/server.js": {
      "maxDuration": 30,
      "memory": 1024,
      "maxRequestBodySize": 10485760
    }
  }
}
